    "@dao-xyz/peerbit-program": "^0.0.2",
    "@dao-xyz/peerbit-store": "^0.0.22",
    "@dao-xyz/peerbit-trusted-network": "^0.0.29",
    "is-node": "^1.0.2",
    "level": "^8.0.0",
    "localstorage-down": "^0.6.7",
    "logplease": "^1.2.14",
    "lru-cache": "^7.14.0",
    "multihashes": "~3.0.1",
    "wherearewe": "^1.0.2"
