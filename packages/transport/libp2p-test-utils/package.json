{
	"name": "@peerbit/libp2p-test-utils",
	"version": "2.1.3",
	"description": "Test utils for Peerbit",
	"sideEffects": false,
	"type": "module",
	"types": "./dist/src/index.d.ts",
	"typesVersions": {
		"*": {
			"*": [
				"*",
				"dist/*",
				"dist/src/*",
				"dist/src/*/index"
			],
			"src/*": [
				"*",
				"dist/*",
				"dist/src/*",
				"dist/src/*/index"
			]
		}
	},
	"files": [
		"src",
		"dist",
		"!dist/test",
		"!**/*.tsbuildinfo"
	],
	"exports": {
		".": {
			"types": "./dist/src/index.d.ts",
			"import": "./dist/src/index.js"
		}
	},
	"eslintConfig": {
		"extends": "ipfs",
		"parserOptions": {
			"project": true,
			"sourceType": "module"
		},
		"ignorePatterns": [
			"!.aegir.js",
			"test/ts-use",
			"*.d.ts"
		]
	},
	"publishConfig": {
		"access": "public"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/dao-xyz/peerbit"
	},
	"engines": {
		"node": ">=16.15.1"
	},
	"author": "dao.xyz",
	"license": "MIT",
	"dependencies": {
		"@dao-xyz/libp2p-noise": "^14.1.2",
		"@libp2p/tcp": "^9.0.15",
		"@libp2p/webrtc": "^4.0.13",
		"@libp2p/websockets": "^8.0.15",
		"@libp2p/identify": "^1.0.12",
		"@libp2p/circuit-relay-v2": "^1.0.13",
		"libp2p": "^1.2.3"
	},
	"scripts": {
		"clean": "aegir clean",
		"build": "aegir build --no-bundle",
		"test": "aegir test -t node"
	},
	"devDependencies": {
		"@peerbit/time": "2.0.6"
	},
	"localMaintainers": [
		"dao.xyz"
	]
}